<script>
    
     $(document.getElementById("divloc")).slideUp(20);
    $(document.getElementById("divlocations")).slideUp(20);
    $(document.getElementById("divUser")).slideUp(20);
    
    $(document.getElementById("divUserlocations")).slideUp(20);
    $("img").hover(function(){
      
        $(this).css("border-color", "#000099");
        $(this).css("cursor", "pointer");
      
        }, function(){
      
        $(this).css("border-color", "transparent");
        });
    
    var useremail = getCookie("EmailAddress");
    var userid = getCookie("UserId");
    if (userid == "") { $(document.getElementById("usrlocClick")).css("visibility", "hidden") } else { $(document.getElementById("usrlocClick")).css("visibility", "visible") };
    if (useremail != ""){ document.getElementById("lblUserClick").innerHTML= useremail;}
</script>